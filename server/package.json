{
  "name": "stylable-intelligence-server",
  "description": "Example implementation of a language server in node.",
  "version": "0.0.1",
  "author": "Nadav Abraami (nadav@wix.com)",
  "publisher": "Nadav Abraami (nadav@wix.com)",
  "license": "MIT",
  "engines": {
    "vscode": "^1.14.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "stylable": "^3.0.0",
    "vscode-languageclient": "^3.3.0",
    "vscode": "^1.1.4",
    "vscode-languageserver": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^6.0.52",
    "typescript": "^2.1.5"
  },
  "scripts": {
    "compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p . && cp installVScode.js ../client/server/",
    "watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc --watch -p .",
    "build": "node installVScode && tsc"
  }
}
